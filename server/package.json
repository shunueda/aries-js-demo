{
  "name": "server",
  "author": "Shun Ueda",
  "license": "MIT",
  "version": "1.0.1",
  "description": "Functional library for JavaScript/Typescript",
  "type": "module",
  "main": "src/index.ts",
  "repository": {
    "url": "https://github.com/shunueda/aleutian.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clone": "cp -r src dist/src",
    "fixImport": "node scripts/fixImports.js dist",
    "prebuild": "yarn clean && mkdir dist && yarn clone && yarn fixImport",
    "build": "swc dist/src -q --out-dir build",
    "start": "yarn build && node build/src",
    "postbuild": "yarn clean"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "@aries-framework/core": "^0.4.1",
    "@aries-framework/indy-sdk": "^0.4.1",
    "@aries-framework/node": "^0.4.1",
    "indy-sdk": "^1.16.0-dev-1655",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.17"
  }
}
